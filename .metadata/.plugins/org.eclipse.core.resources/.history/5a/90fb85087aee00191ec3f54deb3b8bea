package dayseven.quiz;

/**
 *
 *
 * @Author JKermeng
 * @Date 2019Äê10ÔÂ14ÈÕ
 * @Version 1.0.0
 */
public class Calulate {
	private int a = 0;
	private int b = 0;
	
	public Calulate(int a, int b) {
		super();
		this.a = a;
		this.b = b;
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	public int jia() {
		int result = 0;
		result = a + b;
		return result;
	}
	public int jian() {
		int result = 0;
		result = a - b;
		return result;
	}
	public int cheng() {
		int result = 0;
		result = a * b;
		return result;
	}
	public int chu() {
		int result = 0;
		result = a / b;
		return result;
	}

}
