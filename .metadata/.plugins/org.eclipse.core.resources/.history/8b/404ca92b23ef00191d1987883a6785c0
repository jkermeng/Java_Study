public class Potion {

public static void main(String[] args) {
	Scanner scanner = new Scanner(System.in);
	String value = null;
	String values = "y";
	int a[] = new int[6];
	int b[] = {1,5,8,6,2,4};
	for (int i = 1; i < a.length; i++) {
//		System.out.println(b.length);
		if (b.length-i == 0) {
			a[0]=b[0];
		}else {
			a[a.length-i] = b[i];
		}
		
		
//			=b[i];
	}
	for (int i : a) {
		System.out.print(i);
	}
	
	

}
