public class school_work {

	public static void main(String[] args) {
	
		for(int i=1;i<5;i++){
			System.out.println("*");
			for(int j =0; j<i*2;j++)
			{
				System.out.print("*");
			}
			System.out.println("*");
		}
			
	}

}
